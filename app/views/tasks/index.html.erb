<%= link_to '新規タスク追加', new_task_path %>

<% @tasks.each do |task| %>
  <div class = "task">
    <div class = "task-content">
      <%= link_to task.content, task_path(task.id) %>
    </div>
    <%# デッドラインに変えたい %>
    <div class = "task-date">
      投稿日時: <%= task.created_at%>
    </div>
  </div>
<% end %>